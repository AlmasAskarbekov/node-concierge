// Place your settings in this file to overwrite default and user settings.
{
	"files.exclude": {
		"**/.git": true,
		"**/.DS_Store": true,
		"**/archive/*.db": true,
		"**/archive/*.tar": true,
		"**/repositories": true,
		"**/*.js" : { "when": "$(basename).tsx" }
	},
    "typescript.tsdk": "node_modules/typescript/lib",
	"editor.tabSize": 4,
	"files.eol": "\n"
}